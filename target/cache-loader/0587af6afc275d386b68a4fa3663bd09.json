om 'app/shared/model/tablet-view.model';\nimport { TabletViewService } from './tablet-view.service';\n\n@Component({\n  selector: 'jhi-tablet-view-update',\n  templateUrl: './tablet-view-update.component.html'\n})\nexport class TabletViewUpdateComponent implements OnInit {\n  isSaving: boolean;\n\n  editForm = this.fb.group({\n    id: []\n  });\n\n  constructor(protected tabletViewService: TabletViewService, protected activatedRoute: ActivatedRoute, private fb: FormBuilder) {}\n\n  ngOnInit() {\n    this.isSaving = false;\n    this.activatedRoute.data.subscribe(({ tabletView }) => {\n      this.updateForm(tabletView);\n    });\n  }\n\n  updateForm(tabletView: ITabletView) {\n    this.editForm.patchValue({\n      id: tabletView.id\n    });\n  }\n\n  previousState() {\n    window.history.back();\n  }\n\n  save() {\n    this.isSaving = true;\n    const tabletView = this.createFromForm();\n    if (tabletView.id !== undefined) {\n      this.subscribeToSaveResponse(this.tabletViewService.update(tabletView));\n    } else {\n      this.subscribeToSaveResponse(this.tabletViewService.create(tabletView));\n    }\n  }\n\n  private createFromForm(): ITabletView {\n    return {\n      ...new TabletView(),\n      id: this.editForm.get(['id']).value\n    };\n  }\n\n  protected subscribeToSaveResponse(result: Observable<HttpResponse<ITabletView>>) {\n    result.subscribe(() => this.onSaveSuccess(), () => this.onSaveError());\n  }\n\n  protected onSaveSuccess() {\n    this.isSaving = false;\n    this.previousState();\n  }\n\n  protected onSaveError() {\n    this.isSaving = false;\n  }\n}\n"]}]}